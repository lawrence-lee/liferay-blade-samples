<?xml version="1.0" encoding="UTF-8"?>
<component name="com.liferay.blade.samples.language.web.LanguageWebPortlet" immediate="true">
  <implementation class="com.liferay.blade.samples.language.web.LanguageWebPortlet"/>
  <service>
    <provide interface="javax.portlet.Portlet"/>
  </service>
  <property name="javax.portlet.init-param.view-template" type="String" value="/view.jsp"/>
  <property name="javax.portlet.security-role-ref" type="String" value="power-user,user"/>
  <property name="javax.portlet.resource-bundle" type="String" value="content.Language"/>
  <property name="javax.portlet.init-param.template-path" type="String" value="/"/>
  <property name="javax.portlet.name" type="String" value="com_liferay_blade_samples_language_web"/>
  <property name="com.liferay.portlet.instanceable" type="String" value="true"/>
  <property name="javax.portlet.display-name" type="String" value="BLADE Language Web Portlet"/>
  <property name="com.liferay.portlet.display-category" type="String" value="category.sample"/>
</component>
